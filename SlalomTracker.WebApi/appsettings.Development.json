{
  "Logging": {
      "Console": {
          "LogLevel": {
              "Default": "Information",
              "Microsoft": "Warning",
              "Microsoft.Hosting.Lifetime": "Information"
          },
          "FormatterName": "json",
          "FormatterOptions": {
              "SingleLine": true,
              "IncludeScopes": true,
              "TimestampFormat": "MM/dd/yyyy hh:mm:ss.ff",
              "UseUtcTimestamp": true,
              "JsonWriterOptions": {
                  "Indented": false
              }
          }
      }
  },
  "AllowedHosts": "*"
}